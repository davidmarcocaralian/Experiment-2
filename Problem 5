#include <iostream>
#include <conio.h>
#include <string>
#include <iomanip>

using namespace std;

int main()
{

	// Problem 5

	int n = 1;

	cout << "Counting: " << endl;

	while (n <= 10)
	{
		cout << n << ", ";
		n++; 
		if (n == 10)
		{
			while (n <= 30)
			{
				cout << n << ", ";
				n++; n++;
			}
		}
	}
	

	_getch();
	return 0;
}
