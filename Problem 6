#include <iostream>
#include <conio.h>
#include <iomanip>

using namespace std;

int main()
{

	//Problem 6

	int i = 0;
	int j = 1;
	int k = 1;

	cout << "Fibonacci Numbers: " << endl << i << "," << j << ",";

	while (i + j <= 10946)
	{
		k = i + j;
		i = j;
		j = k;
		cout << k << ", ";
	}

	_getch();
	return 0;

}
